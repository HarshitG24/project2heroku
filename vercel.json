{
  "version": 2,
  "name": "quizscript",
  "builds": [{ "src": "./bin/www.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "/public/$1"
    }
  ]
}
